{
    "beautify.config" : "string|Object.<string,string|number|boolean>",
    "beautify.language": {
        "js": {
            "type": ["javascript", "json", "ts"],
            "filename": [".jshintrc", ".jsbeautifyrc"]
        },
        "css": ["css", "scss"],
        "html": ["htm", "html"]
    },
    "editor.formatOnSave": true,
    "beautify.ignore": ["*/test.js", "**/node_modules/*", "**/test/**/*"]
}